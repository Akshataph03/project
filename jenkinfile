        stage('Deploy'){
            steps {
                echo ' Deploy successfully done '
            }
        }
        stage('Test'){
            steps {
                echo 'Test successfully done '
            }
        }
        stage('Release'){
            steps {
                echo ' Release successfully done '
            }
        }
        
    }
}
